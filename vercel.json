    "outputDirectory": "dist",
    "installCommand": "npm install --legacy-peer-deps",
    "devCommand": "npm run dev",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://priv-caster.onrender.com/:path*"
        }
    ]
}